{
  "name": "anime-wordle",
  "version": "1.0.0",
  "description": "anime wordle",
  "author": "OsirisFrik",
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./src/index.html --no-cache",
    "build": "parcel build ./src/index.html --dist-dir build"
  },
  "pwaManifest": {
    "name": "Anime Wordle",
    "shortName": "AW",
    "startUrl": "./",
    "theme": "#581c87",
    "generateIconOptions": {
      "baseIcon": "./src/img/icons/AW.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "dependencies": {
    "@osirisfrik/aniapi-js": "^1.0.1",
    "axios": "^0.26.0",
    "firebase": "^9.6.7",
    "remixicon": "^2.5.0",
    "tailwindcss-dark-mode": "^1.1.7",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.3.2",
    "@parcel/packager-raw-url": "2.3.2",
    "@parcel/packager-xml": "2.3.2",
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.18",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "parcel": "^2.3.2",
    "parcel-config-pwa-manifest": "^0.1.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.6",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.23",
    "tty-browserify": "^0.0.1",
    "typescript": "^4.5.5"
  }
}
